<?xml version="1.0" encoding="gb2312"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!--***********************dao configuration*********************************************** -->
	<bean id="lsEmployeeDao" class="org.loushang.demo.html.dao.jdbc.LsEmployeeJdbcDao">
		<property name="dataSource">
			<ref bean="dataSource" />
		</property>
	</bean>

	<!--***********************domain configuration******************************************** -->

	<bean id="lsEmployeeDomain"
		class="org.loushang.demo.html.domain.support.LsEmployeeDomainImpl">
		<property name="lsEmployeeDao">
			<ref bean="lsEmployeeDao" />
		</property>
	</bean>
	<!--***********************service configuration******************************************* -->

	<bean id="lsEmployeeService"
		class="org.loushang.demo.html.service.support.LsEmployeeServiceImpl">
		<property name="lsEmployeeDomain">
			<ref bean="lsEmployeeDomain" />
		</property>
		<property name="transactionTemplate">
			<ref bean="jdbcTransactionTemplate" />
		</property>
	</bean>


	<!--***********************Flip initialization special query configuration 
		begin****************************************** -->
	<bean id="lsemployeequery_page_init" class="org.loushang.demo.html.query.LsEmployeeQueryPageInitCmd">
		<property name="lsEmployeeService">
			<ref bean="lsEmployeeService" />
		</property>
		<property name="forwards">
			<map>
				<entry key="speQuery">
					<ref bean="lsemployeequeryforward" />
				</entry>
			</map>
		</property>
		<property name="beforIntercept">
			<list>
				<ref bean="ls_employee_interceptor" />
			</list>
		</property>
		<property name="pageSize">
			<value>10</value>
		</property>
	</bean>
	<!--***********************Flip initialization special query configuration 
		end****************************************** -->
	<!--***********************query page forward configuration begin*********************************** -->
	<bean id="lsemployeequeryforward" class="org.loushang.waf.mvc.ServletDispatcherForward">
		<property name="mappings">
			<props>
				<prop key="queryPageInit">jsp/demo/framework/html/ls_employee_query_init.jsp</prop>
			</props>
		</property>
	</bean>
	<!--***********************query page forward configuration end*********************************** -->

	<!--***********************View objects view configuration**************************************** -->
	<bean id="lsEmployeeView" class="org.loushang.demo.html.view.LsEmployeeView"
		singleton="false" />

	<!--***********************cmd configuration begin********************************************* -->
	<bean id="lsemployee" class="org.loushang.demo.html.cmd.LsEmployeeCmd"
		singleton="false">
		<property name="lsEmployeeService">
			<ref bean="lsEmployeeService" />
		</property>
		<property name="taskInfoService">
			<ref bean="sTaskInfoService" />
		</property>
		<property name="view">
			<ref bean="lsEmployeeView" />
		</property>
		<property name="forwards">
			<map>
				<entry key="lsemployeeView">
					<ref bean="lsemployeeforward" />
				</entry>
				<entry key="lsemployeeViewChain">
					<ref bean="lsemployeeforwardchain" />
				</entry>
			</map>
		</property>
		<!--Whether to intercept? -->
		<property name="beforIntercept">
			<list>
				<ref bean="ls_employee_interceptor" />
			</list>
		</property>
	</bean>

	<bean id="ls_employee_interceptor" class="org.loushang.waf.interceptor.CollectionInterceptor">
		<property name="collections">
			<map>
				<entry key="LSEMPLOYEE_EMPLOYEESEX_COLLECTION">
					<ref bean="LSEMPLOYEE_EMPLOYEESEX_COLLECTION" />
				</entry>
				<entry key="LSEMPLOYEE_EMPLOYEEMARRY_COLLECTION">
					<ref bean="LSEMPLOYEE_EMPLOYEEMARRY_COLLECTION" />
				</entry>
				<entry key="LSEMPLOYEE_EMPLOYEEDEGREE_COLLECTION">
					<ref bean="LSEMPLOYEE_EMPLOYEEDEGREE_COLLECTION" />
				</entry>
				<entry key="LSEMPLOYEE_EMPLOYEEPREFERENCE_COLLECTION">
					<ref bean="LSEMPLOYEE_EMPLOYEEPREFERENCE_COLLECTION" />
				</entry>
			</map>
		</property>
	</bean>
	<!--***********************cmd configuration end********************************************* -->

	<!--***********************page forward configuration begin*********************************** -->
	<bean id="lsemployeeforward" class="org.loushang.waf.mvc.ServletDispatcherForward">
		<property name="mappings">
			<props>
				<prop key="detail">jsp/demo/framework/html/ls_employee_detail.jsp</prop>
				<prop key="forinsert">jsp/demo/framework/html/ls_employee_forinsert.jsp</prop>
				<prop key="forupdate">jsp/demo/framework/html/ls_employee_forupdate.jsp</prop>
				<prop key="getProvinces">jsp/demo/framework/html/ls_getProvinces.jsp</prop>
				<prop key="getCitys">jsp/demo/framework/html/ls_getCitys.jsp</prop>
				<prop key="getRoot">jsp/demo/framework/html/tree_node.jsp</prop>
				<prop key="asynDeleteSuccess">jsp/demo/framework/html/asynInformation.jsp</prop>
			</props>
		</property>
	</bean>
	<bean id="lsemployeeforwardchain" class="org.loushang.waf.mvc.ChainForward">
		<property name="mappings">
			<map>
				<entry key="success">
					<ref bean="lsemployeequery_page_init" />
				</entry>
			</map>
		</property>
	</bean>
	<!--***********************page forward configuration end*********************************** -->

	<bean id="LSEMPLOYEE_EMPLOYEESEX_COLLECTION" class="org.loushang.waf.data.EnumCollection">
		<property name="enumName">
			<value>LSEMPLOYEE.EMPLOYEESEX</value>
		</property>
		<property name="enumService">
			<ref bean="enumService" />
		</property>
	</bean>
	<bean id="LSEMPLOYEE_EMPLOYEEMARRY_COLLECTION" class="org.loushang.waf.data.EnumCollection">
		<property name="enumName">
			<value>LSEMPLOYEE.EMPLOYEEMARRY</value>
		</property>
		<property name="enumService">
			<ref bean="enumService" />
		</property>
	</bean>
	<bean id="LSEMPLOYEE_EMPLOYEEDEGREE_COLLECTION" class="org.loushang.waf.data.EnumCollection">
		<property name="enumName">
			<value>LSEMPLOYEE.EMPLOYEEDEGREE</value>
		</property>
		<property name="enumService">
			<ref bean="enumService" />
		</property>
	</bean>
	<bean id="LSEMPLOYEE_EMPLOYEEPREFERENCE_COLLECTION" class="org.loushang.waf.data.EnumCollection">
		<property name="enumName">
			<value>LSEMPLOYEE.EMPLOYEEPREFERENCE</value>
		</property>
		<property name="enumService">
			<ref bean="enumService" />
		</property>
	</bean>

</beans>
